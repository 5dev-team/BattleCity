{
  "name": "client",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "rimraf dist && rimraf ssr-dist && tsc && vite build --config ssr.config.ts && vite build --config vite.config.ts",
    "build:develop": "tsc && vite serve --mode development --open",
    "build:ssr": "tsc && vite build --config ssr.config.ts",
    "start": "yarn run build && yarn run preview",
    "preview": "vite preview",
    "lint": "eslint ./src/**/*.{tsx,ts} --fix && stylelint ./src/**/*.scss --fix",
    "format": "prettier --write .",
    "test": "jest ./"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.9.0",
    "@types/service_worker_api": "0.0.9",
    "axios": "1.2.1",
    "dotenv": "16.0.2",
    "eslint-config-prettier": "8.5.0",
    "history": "5.3.0",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.39.7",
    "react-redux": "8.0.5",
    "react-router-dom": "6.4.2",
    "rimraf": "^4.0.4"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "10.0.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.3.0",
    "@types/jest": "29.2.5",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@types/react-redux": "7.1.24",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "5.35.1",
    "@typescript-eslint/parser": "5.35.1",
    "@vitejs/plugin-react": "2.0.1",
    "eslint": "8.23.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "jest-transform-stub": "2.0.0",
    "lefthook": "1.1.1",
    "prettier": "2.7.1",
    "sass": "1.56.1",
    "stylelint": "14.15.0",
    "stylelint-config-prettier": "9.0.4",
    "stylelint-config-standard": "29.0.0",
    "stylelint-config-standard-scss": "6.1.0",
    "terser": "5.16.0",
    "ts-jest": "29.0.3",
    "typescript": "4.8.2",
    "vite": "3.0.7"
  },
  "license": "MIT"
}
